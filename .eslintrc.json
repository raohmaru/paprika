{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-constant-condition": 0,
      "indent": ["error", 4],
      "semi": "error"
    }
}
